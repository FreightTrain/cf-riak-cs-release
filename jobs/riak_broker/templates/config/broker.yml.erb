username: "<%= p('riak_broker.username') %>"
password: "<%= p('riak_broker.password') %>"
riak_hosts:
<% p('riak_broker.hosts').each do |host| %>
  - "<%= host %>"
<% end %>
riak_cleanup_enabled: "<%= p('riak_broker.cleanup_enabled') %>"
<% if_p('riak_broker.service_instance_limit') do |limit| %>
service_instance_limit: <%= limit %>
<% end %>
